{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "array",
    "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
            "id": {
                "type": "string"
            },
            "rank": {
                "oneOf": [
                    {
                        "type": "number"
                    },
                    {
                        "type": "null"
                    }
                ]
            },
            "disabled": {
                "type": "boolean"
            }
        },
        "required": ["id", "disabled", "rank"]
    }
}