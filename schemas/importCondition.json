{
    "$schema": "http://json-schema.org/draft-07/schema",
    "oneOf": [
        {
            "type": "null"
        },
        {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "channelName": {
                    "type": "string"
                },
                "sortKey": {
                    "type": "string"
                },
                "startDate": {
                    "type": "string"
                },
                "endDate": {
                    "type": "string"
                },
                "clipLength": {
                    "type": "integer"
                }
            },
            "required": [
                "channelName",
                "sortKey",
                "startDate",
                "endDate",
                "clipLength"
            ]
        }
    ]
}